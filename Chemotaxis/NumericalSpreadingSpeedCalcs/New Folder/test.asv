clear all; close all;
n=1000;
[L,Li] = meshgrid(linspace(-10,10,n),linspace(-10,10,n));
s=4;
wedgeProd = zeros(n,1);
for i = 1:length(L)
    wedgeProd(i) = eigenvectors(L(i),s,.5,1.1);
end
plot(L,wedgeProd);
